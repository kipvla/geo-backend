{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "noEmit": false,
    "incremental": false,
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["**/*"]
}
